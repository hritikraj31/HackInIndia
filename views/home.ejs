        <!-- ======= Contact Section ======= -->
    <section id="login" class="contact">
      <div class="container">

        <div class="section-title">
          <h2>Login Signup</h2>
        </div>
      </div>

      <div class="container">
        <div class="row mt-1" style= "justify-content: center;">

          

          <div class="col-lg-8 mt-2 mt-lg-0">
              <div class="row">
                <button class="col-sm" style="border-right: 2px #055194 solid;" onclick='hideform("f1","f2")'>Login</button>
                <button class="col-sm" onclick='hideform("f2","f1")'>Register</button>
              </div>
            <form action="/login" method="POST"  id= 'f1'>
              <div class="container" > 
                <label><b>Username</b></label> 
                <input type="text" placeholder="Enter Username" name="username" id= "username" required> 
      
                <label><b>Password</b></label> 
                <input type="password" placeholder="Enter Password" name="password" id= "password" required> 
                <div class="container">
                    <button type="submit">Login</button> 
                </div>
              </div> 

            </form>
            <form action="/register" method="POST"  id= 'f2' style="display:none;height: 0; width:0;">
                <div class="container" > 
                  <label><b>Username</b></label> 
                  <input type="text" placeholder="Enter Username" name="username" required> 
        
                  <label><b>Password</b></label> 
                  <input type="password" placeholder="Enter Password" name="password" required> 
                  <div class="container">
                      <button type="submit">Sign Up</button> 
                  </div>
                </div> 
  
              </form>

          </div>

        </div>

      </div>
    </section><!-- End Contact Section -->

