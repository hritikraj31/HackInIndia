<script>
    function hideform(f1,f2){
        const fa1= document.getElementById(f1);
        const fa2= document.getElementById(f2);
        fa1.style.display = "block";
        fa2.style.display = "none";
        fa1.style.height = "100%";
        fa1.style.width = "100%";
        fa2.style.height = "0";
        fa2.style.height = "0";
    }
